{
    "version": 2,
    "name": "study-notion",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "node build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}